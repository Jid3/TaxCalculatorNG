{
  "expo": {
    "name": "TaxNaija",
    "slug": "nigerian-tax-calculator",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/iconTaxCalc.png",
    "scheme": "taxcalculator",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash1.png",
      "resizeMode": "contain",
      "backgroundColor": "white"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jideDev.taxnaija",
      "statusBar": {
        "style": "auto",
        "translucent": true
      }
    },
    "android": {
      "package": "com.jideDev.taxnaija",
      "versionCode": 1,
      "permissions": [
        "USE_BIOMETRIC",
        "USE_FINGERPRINT",
        "android.permission.USE_BIOMETRIC",
        "android.permission.USE_FINGERPRINT"
      ],
      "icon": "./assets/images/iconTaxCalc.png",
      "adaptiveIcon": {
        "backgroundColor": "#7CB342",
        "foregroundImage": "./assets/images/foreground.png",
        "monochromeImage": "./assets/images/monochrome.png"
      },
      "splash": {
        "image": "./assets/images/splash1.png",
        "resizeMode": "contain",
        "backgroundColor": "white"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "statusBar": {
        "translucent": true,
        "backgroundColor": "transparent"
      }
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash1.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#000000",
          "dark": {
            "image": "./assets/images/splash1.png",
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-local-authentication",
        {
          "faceIDPermission": "Allow TaxNaija to use Face ID for secure app access."
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "3f54de47-fd39-4a68-b897-ae00890ccfce"
      }
    }
  }
}